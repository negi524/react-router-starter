{
  "name": "react-router-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier app/ stories/ eslint.config.mjs --write",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.2.0",
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "isbot": "^5.1.23",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.21.0",
    "@react-router/dev": "^7.2.0",
    "@storybook/addon-essentials": "^8.6.3",
    "@storybook/addon-onboarding": "^8.6.3",
    "@storybook/blocks": "^8.6.3",
    "@storybook/experimental-addon-test": "^8.6.3",
    "@storybook/react": "^8.6.3",
    "@storybook/react-vite": "^8.6.3",
    "@storybook/test": "^8.6.3",
    "@tailwindcss/vite": "^4.0.9",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/browser": "3.0.7",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.11.4",
    "globals": "^16.0.0",
    "playwright": "^1.50.1",
    "prettier": "3.5.2",
    "react-router-devtools": "^1.1.6",
    "storybook": "^8.6.3",
    "tailwindcss": "^4.0.9",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": ">=23.0.0",
    "npm": "use yarn instead",
    "yarn": "4.7.1"
  },
  "packageManager": "yarn@4.7.0",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
